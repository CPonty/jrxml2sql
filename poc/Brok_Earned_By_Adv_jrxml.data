(string_to_array(ab.brokearn,';'))[6] as "Jun Brok",(string_to_array(ab.brokord,';'))[6] as "Jun Order",
(string_to_array(ab.brokearn,';'))[7] as "Jul Brok",(string_to_array(ab.brokord,';'))[7] as "Jul Order",
(string_to_array(ab.brokearn,';'))[8] as "Aug Brok",(string_to_array(ab.brokord,';'))[8] as "Aug Order",
(string_to_array(ab.brokearn,';'))[9] as "Sep Brok",(string_to_array(ab.brokord,';'))[9] as "Sep Order",
(string_to_array(ab.brokearn,';'))[10] as "Oct Brok",(string_to_array(ab.brokord,';'))[10] as "Oct Order",
(string_to_array(ab.brokearn,';'))[11] as "Nov Brok",(string_to_array(ab.brokord,';'))[11] as "Nov Order",
(string_to_array(ab.brokearn,';'))[12] as "Dec Brok",(string_to_array(ab.brokord,';'))[12] as "Dec Order",

(((string_to_array(ab.brokearn,';'))[12]::numeric) + ((string_to_array(ab.brokearn,';'))[11]::numeric) + ((string_to_array(ab.brokearn,';'))[10]::numeric) +
((string_to_array(ab.brokearn,';'))[9]::numeric) + ((string_to_array(ab.brokearn,';'))[8]::numeric) + ((string_to_array(ab.brokearn,';'))[7]::numeric) +
((string_to_array(ab.brokearn,';'))[6]::numeric) + ((string_to_array(ab.brokearn,';'))[5]::numeric) + ((string_to_array(ab.brokearn,';'))[4]::numeric) +
((string_to_array(ab.brokearn,';'))[3]::numeric) + ((string_to_array(ab.brokearn,';'))[2]::numeric) + ((string_to_array(ab.brokearn,';'))[1]::numeric)) as "Year Brok",

(((string_to_array(ab.brokord,';'))[12]::numeric) + ((string_to_array(ab.brokord,';'))[11]::numeric) + ((string_to_array(ab.brokord,';'))[10]::numeric) +
((string_to_array(ab.brokord,';'))[9]::numeric) + ((string_to_array(ab.brokord,';'))[8]::numeric) + ((string_to_array(ab.brokord,';'))[7]::numeric) +
((string_to_array(ab.brokord,';'))[6]::numeric) + ((string_to_array(ab.brokord,';'))[5]::numeric) + ((string_to_array(ab.brokord,';'))[4]::numeric) +
((string_to_array(ab.brokord,';'))[3]::numeric) + ((string_to_array(ab.brokord,';'))[2]::numeric) + ((string_to_array(ab.brokord,';'))[1]::numeric)) as "Year Ord"

from advisor a

join advbrok ab on ab.advisorid = a.advisorid
join branch b on a.branchid = b.branchid

where ab."market-id" = $P{MARKET}
  and ab.cncycode  = $P{CURRENCY}
  and ab.advyear  >= ($P{YEAR} + (date_part('year',current_date) - date_part('year',cast($P{EXEC_DATE} as date))))
  and ($X{IN, CAST(b.branchid AS VARCHAR), BRANCH_ID} OR $X{IN, '-1', BRANCH_ID}) AND $X{IN, b.branchid, AccessibleSharesBranchIds}
  and ($X{IN, CAST(a.advisorid AS VARCHAR), ADV_CODE} OR $X{IN, '-1', ADV_CODE}) AND $X{IN,a.advisorid, AccessibleSharesAdvisorIds}
  and ($X{IN,a.orgcode, ORG_CODE} OR $X{IN,'ALL',ORG_CODE}) AND $X{IN,a.orgcode,AccessibleOrganisationCodes}

 order by a.orgcode, b.branchid, a.advcode, ab.advyear
